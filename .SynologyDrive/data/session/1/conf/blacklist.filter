[Version]
major = 1
minor = 1

[Common]
black_dir_prefix = "/ISOs", "/Pictures", "/YT", "/old", "/auth-codes", "/txt"
max_length = 0
max_path = 0

[File]
black_suffix = ".ez", ".lnk", ".pst", ".swp", ".temp", ".tmp"
max_size = 0

[Directory]
black_name = "node_modules", "build", ".gradle", "target", ".build", "mysql", "__pycache__", "venv", "_build", "deps", "zig-cache", "zig-out", ".svelte-kit", ".direnv", "result"

[EA]

